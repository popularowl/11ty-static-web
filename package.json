{
  "name": "11ty-minimal-web",
  "version": "1.0.0",
  "description": "minimal 11ty web static site, clean design included",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx @11ty/eleventy",
    "build:dev": "ELEVENTY_ENV=dev && build",
    "build:prod": "ELEVENTY_ENV=prod && build",
    "clean": "rimraf dist",
    "dev": "rimraf dist && npx @11ty/eleventy --watch --serve --incremental",
    "debug": "DEBUG=Eleventy* npm run dev"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "11ty rss",
    "markdown",
    "11ty sitemaps",
    "11ty alpine.js"
  ],
  "author": {
    "name": "Saulius Zukauskas",
    "url": "https://www.popularowl.com",
    "email": "saul@popularowl.com"
  },
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "3.0.0",
    "@11ty/eleventy-fetch": "^4.0.1",
    "@11ty/eleventy-img": "^5.0.0",
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@alpinejs/intersect": "^3.14.3",
    "alpinejs": "^3.14.3",
    "browserslist": "^4.24.2",
    "eleventy-plugin-embed-everything": "^1.19.0",
    "html-minifier-terser": "^7.2.0",
    "lightningcss": "^1.27.0",
    "lodash.get": "^4.4.2",
    "luxon": "3.5.0",
    "mime-types": "^2.1.35",
    "node-html-to-image": "^5.0.0",
    "rimraf": "^6.0.1",
    "terser": "^5.36.0"
  }
}
